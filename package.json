{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "2.1.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "node dist/server/index.js",
    "start:prod": "node dist/server/index.js",
    "build": "vite build && npm run build:backend",
    "build:backend": "tsc --project tsconfig.server.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "seed:courses": "tsx prisma/seed_courses.ts",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "server": "node dist/server/index.js",
    "server:js": "node server/index.js",
    "server:dev": "nodemon --exec \"tsx\" server/index.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:generate": "prisma generate",
    "db:clean": "node scripts/clean-prisma.cjs",
    "db:clean:full": "node scripts/clean-prisma.cjs --full",
    "db:regenerate": "node scripts/clean-prisma.cjs && prisma generate",
    "db:fix-url": "node scripts/fix-database-url.cjs",
    "dev:check": "node scripts/check-dev-server.cjs",
    "dev:network": "node scripts/start-dev-with-network.cjs",
    "dev:fix-firewall": "node scripts/fix-firewall.cjs",
    "dev:mobile": "node scripts/start-with-ngrok.cjs",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "node prisma/seed.cjs",
    "fix:courses": "node scripts/fix-course-assignments.cjs",
    "fix:professor-courses": "node scripts/fix-professor-courses.cjs",
    "update-schedule": "node scripts/update-schedule.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit:new": "jest tests/time.utils.test.ts",
    "check:prod": "node scripts/check-production-readiness.js",
    "test:smoke": "node scripts/smoke-test.js",
    "env:check": "node -e \"const fs=require('fs'); const path=require('path'); const filePath=path.join('scripts','check-env.cjs'); if(fs.existsSync(filePath)){require(path.resolve(filePath));}else{console.log('⚠️  scripts/check-env.cjs not found, skipping...'); process.exit(0);}\"",
    "env:create": "node -e \"const fs=require('fs'); const path=require('path'); const filePath=path.join('scripts','create-env.cjs'); if(fs.existsSync(filePath)){require(path.resolve(filePath));}else{console.log('⚠️  scripts/create-env.cjs not found, skipping...'); process.exit(0);}\"",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:deps": "npx depcheck",
    "lighthouse": "lighthouse http://localhost:5180 --output html --output-path ./reports/lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "postinstall": "prisma generate",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:fly": "fly deploy",
    "docker:build": "docker build -t smart-campus-assistant .",
    "docker:run": "docker run -p 3001:3001 --env-file .env smart-campus-assistant"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.5",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@vitejs/plugin-react": "^5.1.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "c12": "^3.3.1",
    "cmdk": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "fraction.js": "^5.3.4",
    "framer-motion": "^11.18.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.344.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "openai": "^6.3.0",
    "path-browserify": "^1.0.1",
    "pathe": "^2.0.3",
    "prisma": "^6.18.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^6.30.2",
    "recharts": "^3.2.1",
    "redis": "^5.8.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "string-similarity": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.20.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.9.1",
    "@lhci/cli": "^0.12.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.8.4",
    "dompurify": "^3.0.8",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "supertest": "^7.0.0",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-pwa": "^1.1.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.4.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed_courses.ts"
  }
}